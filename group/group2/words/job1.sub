# Run parallel jobs to sum each of the several data.txt.* partial files.

universe = vanilla
proc=$INT(Process,%02d)
log = log 
error = err 

executable = ./job1sort.sh
arguments = data.txt.$(proc)
output = out 

should_transfer_files = YES
when_to_transfer_output = ON_EXIT
transfer_input_files = data.txt.$(proc)

request_cpus = 1
request_memory = 1GB
request_disk = 1MB

queue 5
